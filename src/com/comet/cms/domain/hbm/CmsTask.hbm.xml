<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.comet.cms.domain">
    <class
            name="CmsTask"
            table="CMS_TASK"
            >

        <meta attribute="sync-DAO">false</meta>
        <meta attribute="sync-jsp">true</meta>
        <meta attribute="sync-boolean">get</meta>
        <meta attribute="treeable">false</meta>
        <meta attribute="projectable">false</meta>
        <meta attribute="tableName">接受任务列表</meta>
        <meta attribute="tableComment">接受任务列表</meta>
        <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system">oa</meta>

        <id
                name="id"
                type="java.lang.Long"
                column="ID"
                >
            <generator class="com.comet.core.orm.hibernate.LongIdGenerator"/>
        </id>

        <property
                name="handlingSuggestion"
                column="HANDLING_SUGGESTION"
                type="string"
                not-null="false"
                length="2000"
                >
            <meta attribute="ColumnName">处理意见</meta>
            <meta attribute="ColumnComment">处理意见</meta>
        </property>

        <property
                name="handlingResult"
                column="HANDLING_RESULT"
                type="string"
                not-null="false"
                length="2"
                >
            <meta attribute="ColumnName">处理结果</meta>
            <meta attribute="ColumnComment">处理结果</meta>
        </property>

        <property
                name="isRead"
                column="IS_READ"
                type="java.lang.Boolean"
                not-null="false"
                length="1"
                >
            <meta attribute="ColumnName">是否已读</meta>
            <meta attribute="ColumnComment">是否已读</meta>
        </property>

        <property
                name="handlingTime"
                column="HANDLING_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">处理时间</meta>
            <meta attribute="ColumnComment">处理时间</meta>
        </property>

        <property
                name="readTime"
                column="READ_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">阅读时间</meta>
            <meta attribute="ColumnComment">阅读时间</meta>
        </property>

        <property
                name="createTime"
                column="CREATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

        <property
                name="updateTime"
                column="UPDATE_TIME"
                type="timestamp"
                not-null="false"
                length="19"
                >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

        <property
                name="createUser"
                column="CREATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人(记录帐号）</meta>
        </property>

        <property
                name="updateUser"
                column="UPDATE_USER"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人(记录帐号）</meta>
        </property>

        <many-to-one
                name="article"
                column="ARTICLE_ID"
                class="CmsArticle"
                not-null="false"
                >
        </many-to-one>

        <many-to-one
                name="user"
                column="USER_ID"
                class="com.comet.system.domain.SysUser"
                not-null="false"
                >
        </many-to-one>

    </class>
    <query name="cmstask_all_data_query">
        <![CDATA[FROM CmsTask ]]>
    </query>
</hibernate-mapping>